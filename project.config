[project]
	description = Rammer is a framework dedicated to build high performance async API servers on top of non-blocking (asynchronous) Ruby web server called Goliath. 
[access "refs/*"]
	owner = group user/qburstruby (qburstruby)
[access "refs/for/refs/*"]
	read = group user/qburstruby (qburstruby)
	push = group user/qburstruby (qburstruby)
	label-Code-Review = -2..+2 group user/qburstruby (qburstruby)
