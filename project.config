	rebase = group user/qburstruby (qburstruby)
	removeReviewer = group user/qburstruby (qburstruby)
	submit = group user/qburstruby (qburstruby)
[access "refs/heads/*"]
	read = group user/qburstruby (qburstruby)
	create = group user/qburstruby (qburstruby)
	push = +force group user/qburstruby (qburstruby)
	pushMerge = group user/qburstruby (qburstruby)
[access "refs/tags/*"]
	push = +force group user/qburstruby (qburstruby)
	pushTag = group user/qburstruby (qburstruby)
